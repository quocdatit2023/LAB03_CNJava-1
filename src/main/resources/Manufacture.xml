<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="model.Manufacture">
        <table name="manufacture" schema="lab03"/>
        <attributes>
            <id name="id">
                <column name="id"/>
            </id>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="location">
                <column name="location"/>
            </basic>
            <basic name="employee">
                <column name="employee" precision="11"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
